<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>A2_HRManagement</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>Menu</servlet-name>
		<servlet-class>ca.myseneca.controller.Menu</servlet-class>
		<init-param>
			<param-name>initNum</param-name>
			<param-value>0</param-value>
		</init-param>
		
	</servlet>
	<servlet>
		<servlet-name>EmployeeList</servlet-name>
		<servlet-class>ca.myseneca.controller.EmployeeList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeList</servlet-name>
		<url-pattern>/addEmployee</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeList</servlet-name>
		<url-pattern>/searchEmployee</url-pattern>
	</servlet-mapping>
</web-app>